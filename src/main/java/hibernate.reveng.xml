<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-schema="healthcare" match-name=".*" />
	<table-filter match-catalog="healthcare" match-name="clinic" />
	<table-filter match-catalog="healthcare" match-name="doctor" />
	<table-filter match-catalog="healthcare" match-name="message" />
	<table-filter match-catalog="healthcare" match-name="patient" />
	<table-filter match-catalog="healthcare" match-name="questionmessage" />
	<table-filter match-catalog="healthcare" match-name="reservation" />
	<table-filter match-catalog="healthcare" match-name="schedules" />
	<table-filter match-catalog="healthcare" match-name="specialty" />
	<table-filter match-catalog="healthcare" match-name="users" />
</hibernate-reverse-engineering>